speed(0)

# Move to bottom row placement at (-100,-200)
penup()
setposition(-100,-200)

# Draw 3 circles for bottom row
for i in range(3):
    pendown()
    circle(50)
    penup()
    forward(100)
    
setposition(-50,-100)   
for i in range(2):
    pendown()
    circle(50)
    penup()
    forward(100)
    
setposition(0,0)   
for i in range(1):
    pendown()
    circle(50)
    penup()